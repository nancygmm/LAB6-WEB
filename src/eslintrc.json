{
    "extends": "airbnb-base",
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "import/extensions": ["error", "ignorePackages"],
      "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }],
      "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
      "no-console": "off"
    }
  }
  